<h1>Add Song</h1>
<%= form_for [@billboard, @song] do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :artist %>
    <%= f.select :artist_id, @artists.collect { |u| [u.name, u.id] } %>
  </div>
  <br />
  <%= f.submit %>
<%end%>
<p>If you dont see the artist click <%= link_to 'here', new_artist_path %></p>
